<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="720.0" prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/21">
    <children>
        <Button fx:id="CreateGameButton" alignment="CENTER" layoutX="51.0" layoutY="122.0" mnemonicParsing="false"
                prefHeight="27.0" prefWidth="141.0" text="Create Game">
            <font>
                <Font name="Bell MT" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="nicknameButton" alignment="CENTER" layoutX="51.0" layoutY="200.0" mnemonicParsing="false"
                prefHeight="27.0" prefWidth="141.0" text="Change Nickname">
            <font>
                <Font name="Bell MT" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="BackTitleButton" layoutX="51.0" layoutY="278.0" mnemonicParsing="false"
                prefHeight="27.0" prefWidth="141.0" text="Back to title screen">
            <font>
                <Font name="Bell MT" size="14.0"/>
            </font>
        </Button>
        <ScrollPane fx:id="lobbiesPane" layoutX="221.0" layoutY="25.0" prefHeight="599.0" prefWidth="1400.0">
            <content>
                <VBox fx:id="anchor" minHeight="0.0" minWidth="0.0" prefHeight="601.0" prefWidth="1402.0"/>
            </content>
        </ScrollPane>
        <ComboBox fx:id="maxPlayersInput" prefWidth="150.0" promptText="Select maximum number of players:"/>
        <Label fx:id="players" alignment="CENTER" text="Inserire il numero di giocatori">
            <font>
                <Font name="Bell MT" size="30.0"/>
            </font>
        </Label>
        <Button fx:id="okPlayers" mnemonicParsing="false"
                prefHeight="27.0" prefWidth="141.0" text="OK">
            <font>
                <Font name="Bell MT" size="14.0"/>
            </font>
        </Button>
        <Label fx:id="profile" alignment="TOP_RIGHT">
            <font>
                <Font name="Bell MT" size="14.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
