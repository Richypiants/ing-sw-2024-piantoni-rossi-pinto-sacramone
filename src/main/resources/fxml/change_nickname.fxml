<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:id="languagePane" maxHeight="-Infinity" maxWidth="-Infinity"
           minHeight="-Infinity"
           minWidth="-Infinity" xmlns="http://javafx.com/javafx/21">
    <children>
        <Label fx:id="error" prefWidth="800.0" style="-fx-alignment: Center"
               textAlignment="CENTER" textFill="#000000">
            <font>
                <Font name="Bell MT" size="14.0"/>
            </font>
        </Label>
        <Button fx:id="nicknameButton" alignment="CENTER" mnemonicParsing="false"
                prefHeight="38.0" prefWidth="323.0" text="OK"
                textAlignment="JUSTIFY" wrapText="true">
        </Button>
        <Label fx:id="nicknameLabel" alignment="CENTER" layoutX="198.0" layoutY="115.0"
               text="Inserisci un nuovo nickname"
               textFill="#000000">
            <font>
                <Font name="Bell MT" size="30.0"/>
            </font>
        </Label>
        <TextField fx:id="nicknameField" prefHeight="10.0" prefWidth="50.0">
            <font>
                <Font name="Bell MT" size="20.0"/>
            </font>
        </TextField>
    </children>
</StackPane>